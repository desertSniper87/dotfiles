!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

!Xft.dpi:                    96
!Xft.antialias:              false
!Xft.rgba:                   rgb
!Xft.hinting:                true
!Xft.hintstyle:              hintslight

!-------------------------------------------------------------------------------
! URxvt settings
! Colours lifted from Solarized (http://ethanschoonover.com/solarized)
! More info at:
! http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod
!-------------------------------------------------------------------------------

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.transparent:          false
URxvt.fading:               0
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.saveLines:            4096
URxvt.internalBorder:       3
URxvt.lineSpace:            0

! Fonts
URxvt.allow_bold:           true
!URxvt*font:                 -*-terminus-*-12-120-72-72-c-60-iso8859-1
!URxvt*font:                 -*-terminus-bold-*-*-*-14-*-*-*-*
!URxvt*font:                 xft:bitstream vera sans Mono:size=12:antialias=true
!URxvt*boldFont:             xft:bitstream vera sans Mono:style=bold:size=12:antialias=true
!URxvt.font:                 -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
!URxvt*boldFont:             xft:bitstream vera sans Mono:size=12:antialias=true
!URxvt*font:                 xft:dejavu sans Mono:size=12:antialias=true
!URxvt*boldFont:             xft:dejavu sans Mono:size=12:antialias=true
!URxvt*font:                 xft:droid sans mono for powerline:size=10:antialias=true
!URxvt*font:                 xft:droid sans mono for powerline:style=bold:size=10:antialias=true
!URxvt*boldFont:             xft:droid sans mono for powerline:style=bold:size=10:antialias=true
!URxvt*font:                 xft:source code pro:size=12:antialias=true
!URxvt*boldFont:             xft:source code pro:size=12:antialias=true
!URxvt*font:                 xft:ubuntu mono:size=12:antialias=true
!URxvt*boldFont:             xft:ubuntu mono bold:size=12:antialias=true
!URxvt*font:                 xft:inconsolata regular:size=10:antialias=true
!URxvt*boldFont:             xft:inconsolata regular:size=10:antialias=true
!URxvt*font:                 xft:monaco:size=12:antialias=true
!URxvt*boldFont:             xft:monaco:size=12:antialias=true
!urxvt*font:                 -misc-terminusmodx.icons-medium-r-normal-*-12-120-72-72-c-60-iso8859-1
!URxvt*font:                 xft:monaco\ for\ powerline:size=12:antialias=true
!URxvt*boldFont:             xft:monaco\ for\ powerline:size=12:antialias=true
!URxvt*font :                xft:Tamzenforpowerline:style=bold:size=8
URxvt*font :                xft:ProFont\ for powerline:size=12
URxvt*boldFont :            xft:ProFont for powerline:style=bold:size=12
!URxvt*font :                xft:Terminess\ powerline\ bold:size=12
!URxvt*font :                xft:Roboto mono for powerline:size=12
!URxvt*boldFont :            xft:Roboto mono for powerline:style=bold:size=12
!URxvt*font :                -misc-tamzenforpowerline-bold-r-normal--12-87-100-100-c-60-iso10646-1
!URxvt*font :                : -misc-tamzenforpowerline-medium-r-normal--16*
!URxvt*boldFont:             xft:Tamzenforpowerline\ bold:size=12:antialias=true

!URxvt.keysym.C-Down:        command:\033]710;-*-dina-medium-r-normal-12-*-*-*-*-*-*-*\007
URxvt.keysym.M-C-Down:      command:\033]710; -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1\007
!URxvt.keysym.M-C-Down:      command:\033]710;xft:terminus:pixelsize=14\007
!URxvt.keysym.C-Down:        command:\033]710;xft:Dina:pixelsize=13\007
!URxvt.keysym.C-Up:          command:\033]710;xft:bitstream vera sans Mono:pixelsize=16:antialias=true\007
!URxvt.keysym.C-Up:          command:\033]710;xft:Ubuntu\ Mono\ Derivative\ Powerline:style=bold:pixelsize=16:antialias=true\007
!URxvt.keysym.C-Up:          command:\033]710;xft:dejavu sans Mono:antialias=true\007
!URxvt.keysym.M-C-Up:        command:\033]710;xft:monaco:antialias=true\007
URxvt.keysym.M-C-Up:        command:\033]710;xft:monaco\ for\ powerline:size=12:antialias=true\007


! Fix font space
!URxvt*letterSpace: 0

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Perl extensions1
! URxvt.perl-lib:             /home/torsho/urxvt-perls
URxvt.matcher.button:       1
URxvt.urlLauncher:          qutebrowser
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,resize-font,matcher,vtwheel
URxvt.url-select.launcher:  qutebrowser
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

!URxvt.keysym.C-Up:     font-size:increase
!URxvt.keysym.C-Down:   font-size:decrease
!URxvt.keysym.C-S-Up:   font-size:incglobal
!URxvt.keysym.C-S-Down: font-size:decglobal

!URxvt.resize-font.smaller: C-Down
!URxvt.resize-font.bigger: C-Up

URxvt.clipboard.autocopy:   true
URxvt.keysym.M-c:           perl:clipboard:copy
URxvt.keysym.M-v:           perl:clipboard:paste
URxvt.keysym.M-C-v:         perl:clipboard:paste_escaped


! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true

! ScrollBack Buffer
URxvt*saveLines:            4095

!! do not scroll with output
!URxvt*scrollTtyOutput: false

